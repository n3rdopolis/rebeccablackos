[Desktop Entry]
Enabled=1
Type=WSession
Name=Orbital
Comment=The Orbital Desktop Shell
CompositorExec=/opt/bin/orbital
CompositorSupportsKMS=1
CompositorSupportsWL=0
CompositorKMSArguments=--backend=drm-backend
CompositorWLArguments=--backend=wayland-backend
CompositorDisplayArgument=--socket 
SessionArgument=
CompositorKMSSeatAware=1
NeedsFile=/opt/bin/orbital
[EnvironmentFile]
set=QT_WAYLAND_FORCE_DPI=100
